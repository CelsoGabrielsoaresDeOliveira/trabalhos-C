#include <stdio.h>

int main() {
    int numero, contagem = 0;

    printf("Digite dez numeros inteiros:\n");
    

    for (int i = 0; i < 10; i++) {
        scanf("%d", &numero);
        

        if (numero % 5 == 0 && numero % 3 == 0) {
            contagem++;
        }
    }

    // Imprime o resultado
    printf("Total de numeros divisiveis por 5 e 3 ao mesmo tempo: %d\n", contagem);

    return 0;
}
